{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "isolatedModules": true,
    "jsx": "react-jsx",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext",
      "WebWorker"
    ],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "noEmit": true,
    "paths": {
      "~/*": [
        "./app/*"
      ]
    },
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "ESNext",
    "types": [
      "unplugin-icons/types/react"
    ]
  },
  "include": [
    "icons.d.ts",
    "env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "**/*.test.ts"
  ]
}