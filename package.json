    "@js-joda/core": "^5.6.1",
    "@js-joda/locale_en-us": "^4.8.11",
    "@js-joda/timezone": "^2.18.2",
    "@localfirst/auth": "^5.4.1",
    "@localfirst/auth-provider-automerge-repo": "^5.4.1",
    "@localfirst/auth-syncserver": "^5.4.1",
    "@localfirst/crypto": "^5.4.1",
    "@playwright/test": "^1.42.0",
    "@remix-run/node": "^2.8.0",
    "@remix-run/react": "^2.8.0",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
