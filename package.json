    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "unplugin-auto-import": "^0.17.5",
    "vite": "5.1.4",
    "vite-plugin-pwa": "^0.17.5",
    "vite-plugin-top-level-await": "^1.4.1",
    "vite-plugin-wasm": "^3.3.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^1.3.1",
    "xo": "^0.56.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}