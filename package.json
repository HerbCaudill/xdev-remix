{
  "name": "xdev-remix",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "run-p dev:open dev:syncserver",
    "preview": "vite preview --port 3000",
    "start": "npx sirv-cli build/client/ --single --port 3000",
    "preinstall": "npx only-allow pnpm",
    "dev:start": "remix vite:dev --port 3000",
    "dev:open": "pnpm dev:start --open",
    "dev:syncserver": "cross-env DEBUG=localfirst*,automerge* NODE_ENV=development node ./scripts/syncserver.js",
    "lint": "xo",
    "test": "vitest",
    "test:pw": "pnpm playwright test --project=chromium",
    "test:pw:ui": "cross-env PLAYWRIGHT_UI=1 run-p dev:start dev:syncserver test:pw:exec:ui",
    "test:pw:exec:ui": "pnpm playwright test --project=chromium --ui"
  },
  "dependencies": {
    "@automerge/automerge": "^2.1.10",
    "@automerge/automerge-repo": "^1.1.0",
    "@automerge/automerge-repo-network-broadcastchannel": "^1.1.0",
    "@automerge/automerge-repo-network-websocket": "^1.1.0",
    "@automerge/automerge-repo-react-hooks": "^1.1.0",
    "@automerge/automerge-repo-storage-indexeddb": "^1.1.0",
    "@automerge/automerge-repo-storage-nodefs": "^1.1.0",
    "@headlessui/react": "^1.7.9",
    "@ibm/plex": "^6.1.1",
    "@iconify-json/tabler": "^1.1.105",
    "@js-joda/core": "^5.5.2",
    "@js-joda/locale_en-us": "^4.8.6",
    "@js-joda/timezone": "^2.17.2",
    "@localfirst/auth": "^5.4.1",
    "@localfirst/auth-provider-automerge-repo": "^5.4.1",
    "@localfirst/auth-syncserver": "^5.4.1",
    "@localfirst/crypto": "^5.4.1",
    "@playwright/test": "^1.40.0",
    "@remix-run/node": "^2.7.2",
    "@remix-run/react": "^2.7.2",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@uidotdev/usehooks": "^2.4.1",
    "classnames": "^2.3.2",
    "clipboardy": "^2.3.0",
    "csv-parse": "^5.3.5",
    "debug": "^4.3.4",
    "dexie": "^3.2.3",
    "dexie-react-hooks": "^1.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hotkeys-hook": "^4.3.7",
    "react-qr-code": "^2.0.12",
    "react-textarea-autosize": "^8.4.0",
    "ua-parser-js": "^1.0.37",
    "unplugin-icons": "^0.18.5",
    "workbox-core": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.7.2",
    "@types/debug": "^4.1.12",
    "@types/node": "^20.9.4",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/ua-parser-js": "^0.7.39",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "eslint": "^8.33.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-react": "^0.27.0",
    "eslint-plugin-react": "^7.32.2",
