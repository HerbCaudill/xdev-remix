{
  "name": "xdev-remix",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "run-p dev:open dev:syncserver",
    "preview": "vite preview --port 3000",
    "start": "npx sirv-cli build/client/ --single --port 3000",
    "preinstall": "npx only-allow pnpm",
    "dev:start": "remix vite:dev --port 3000",
    "dev:open": "pnpm dev:start --open",
    "dev:syncserver": "cross-env DEBUG=localfirst*,automerge* NODE_ENV=development node ./scripts/syncserver.js",
    "lint": "xo",
    "test": "vitest",
    "test:pw": "pnpm playwright test --project=chromium",
    "test:pw:ui": "cross-env PLAYWRIGHT_UI=1 run-p dev:start dev:syncserver test:pw:exec:ui",
    "test:pw:exec:ui": "pnpm playwright test --project=chromium --ui",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@automerge/automerge": "^2.1.10",
